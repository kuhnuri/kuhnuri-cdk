  "dependencies": {
    "@aws-cdk/aws-apigateway": "^1.0.0",
    "@aws-cdk/aws-batch": "^1.0.0",
    "@aws-cdk/aws-dynamodb": "^1.0.0",
    "@aws-cdk/aws-ec2": "^1.0.0",
    "@aws-cdk/aws-ecr": "^1.0.0",
